  <query>
    <text>
      <![CDATA[
if dbo.FastBusiness$Function$CheckSQLInjection(@stt_rec) = 0 return
]]>&GLTranReportSql;<![CDATA[
else begin
	declare @m_ma_nt0 varchar(10), @dv_tien nvarchar(16), @key nvarchar(128), @bilingual tinyint, @l char(1)
  if @@form in ('011', '021', '031', '041', '051', '061') select @bilingual = 1, @l = 'V' else select @bilingual = 0, @l = @@language
	select @dv_tien = ra_2 from dmnt where ma_nt = @m_ma_nt0

	if @@form in('050', '060', '070', '080', '088', '150', '160', '051', '061') begin
    declare @h_line1 as nvarchar(128), @h_line2 as nvarchar(128), @h_line3 as nvarchar(128), @h_line4 as nvarchar(128)
    if @bilingual = 0 begin
      select @h_line1 = case when @@language = 'V' then cname else cname2 end from reports where ccode = 'DEC005'
      select @h_line2 = case when @@language = 'V' then cname else cname2 end from reports where ccode = 'DEC006'
      select @h_line3 = case when @@language = 'V' then cname else cname2 end from reports where ccode = 'DEC007'
      select @h_line4 = case when @@language = 'V' then cname else cname2 end from reports where ccode = 'DEC008'
    end

		select rtrim(a.ong_ba) as ong_ba,rtrim(b.ten_kh) as ten_kh ,rtrim(b.ten_kh2) as ten_kh2, a.dia_chi, a.dien_giai, a.t_tt, a.t_tt_nt, a.ma_nt,
			  dbo.FastBusiness$Function$System$ReadCurrency(@m_ma_nt0, a.t_tt, @l) as t_tt_in_word,
		    dbo.FastBusiness$Function$System$ReadCurrency(a.ma_nt, a.t_tt_nt, @l) as t_tt_nt_in_word,
        dbo.FastBusiness$Function$System$ReadCurrency(@m_ma_nt0, a.t_tt, 'E') as t_tt_in_word2,
		    dbo.FastBusiness$Function$System$ReadCurrency(a.ma_nt, a.t_tt_nt, 'E') as t_tt_nt_in_word2,
		    a.so_ct_goc as so_ct_goc, a.dien_giai_ct_goc, a.ma_nk, a.so_ct, a.tk, a.ty_gia,
		    replace(str(day(ngay_ct),2),' ','0') ngay, replace(str(month(ngay_ct),2),' ','0') thang, str(year(ngay_ct),4) nam, stt_rec,
        @h_line1 as h_line1, @h_line2 as h_line2, @h_line3 as h_line3, @h_line4 as h_line4
	    from @@prime$partition$current a with(nolock)
		    left join dmkh b with(nolock) on a.ma_kh = b.ma_kh
	    where stt_rec = @stt_rec

			declare @strSQL nvarchar(4000)
			set @strSQL = 'select a.tk_no, sum(a.tien) as tien, sum(a.tien_nt) as tien_nt'
			set @strSQL = @strSQL + ' from (select 0 as l1, line_nbr, tk_no, tt - thue as tien, tt_nt - thue_nt as tien_nt from ' + dbo.ff30_DTable('d56$$partition$current') + ' a with(nolock) where stt_rec = ''' + @stt_rec + ''''
			set @strSQL = @strSQL + char(13) + 'union all'
			set @strSQL = @strSQL + char(13) + 'select 1, line_nbr, tk_thue_no, t_thue, t_thue_nt from r30$$partition$current a with(nolock) where stt_rec = ''' + @stt_rec + ''''
			set @strSQL = @strSQL + ' ) a'
			set @strSQL = @strSQL + ' group by a.tk_no order by min(a.l1), min(a.line_nbr)'
			exec sp_executesql @strSQL
	end else begin
		if @@form in('030', '040', '130', '140', '031', '041') begin
    select @m_ma_nt0 = val from options where name = N'm_ma_nt0'
    select rtrim(a.ong_ba) as ong_ba,rtrim(b.ten_kh) as ten_kh ,rtrim(b.ten_kh2) as ten_kh2, a.dia_chi, a.dien_giai, a.t_tt, a.t_tt_nt, a.ma_nt, 
		    dbo.FastBusiness$Function$System$ReadCurrency(@m_ma_nt0, a.t_tt, @l) as t_tt_in_word, 
		    dbo.FastBusiness$Function$System$ReadCurrency(a.ma_nt, a.t_tt_nt, @l) as t_tt_nt_in_word,
        dbo.FastBusiness$Function$System$ReadCurrency(@m_ma_nt0, a.t_tt, 'E') as t_tt_in_word2, 
		    dbo.FastBusiness$Function$System$ReadCurrency(a.ma_nt, a.t_tt_nt, 'E') as t_tt_nt_in_word2,
		    a.so_ct_goc as so_ct_goc, a.dien_giai_ct_goc, a.ma_nk, a.so_ct, a.tk, a.ty_gia,
		    replace(str(day(ngay_ct),2),' ','0') ngay, replace(str(month(ngay_ct),2),' ','0') thang, str(year(ngay_ct),4) nam, stt_rec
	    from @@prime$partition$current a with(nolock)
		    left join dmkh b with(nolock) on a.ma_kh = b.ma_kh
	    where stt_rec = @stt_rec
		  select @key = 'a.stt_rec = ''' + @stt_rec + ''''
			exec rs_PrintCDTran2 @@language, @key, 'd56$$partition$current', 'r30$$partition$current', @@id
			return
		end
   select a.ma_kh, a.dien_giai, ltrim(a.so_ct) as so_ct, a.ty_gia,
	    a.t_tt, a.t_tt_nt, isnull(b.ten_nh,'') as tennh, isnull(b.ten_nh2,'') as tennh2,
	    isnull(b.tknh,'') as tknh, isnull(b.tinh_thanh,'') as tinhthanh, a.ma_nt, N'1' as lien,
		  isnull(c.ten_kh, '') as ten_kh, isnull(c.tk_nh,'') as tk_nh, isnull(c.ngan_hang,'') as ngan_hang, isnull(c.tinh_thanh,'') as tinh_thanh,
	    dbo.FastBusiness$Function$System$ReadCurrency(@m_ma_nt0, a.t_tt, @l) as t_tt_in_word,
	    dbo.FastBusiness$Function$System$ReadCurrency(a.ma_nt, a.t_tt_nt, @l) as t_tt_nt_in_word,
      dbo.FastBusiness$Function$System$ReadCurrency(@m_ma_nt0, a.t_tt, 'E') as t_tt_in_word2,
	    dbo.FastBusiness$Function$System$ReadCurrency(a.ma_nt, a.t_tt_nt, 'E') as t_tt_nt_in_word2,
	    replace(str(day(ngay_ct),2),' ','0') ngay, replace(str(month(ngay_ct),2),' ','0') thang, cast(year(ngay_ct) as char(4)) nam, k.ten_cty as ten_dvcs, k.dc_cty as dc
    from @@prime$partition$current a with(nolock)
	    left join zcdmtknh b with(nolock) on a.tk = b.tk and a.ma_dvcs = b.ma_dvcs
		left join p56 c with(nolock) on a.stt_rec = c.stt_rec
		left join dmdvcskb k with(nolock) on a.ma_dvcs = k.ma_dvcs
    where a.stt_rec = @stt_rec
	end
  
  select cast(null as image) as logo
		, case when @@form = '270' or @@form = '271' then 'ACB.png'
					when @@form = '272' or @@form = '273' then 'SHB.png'
					when @@form = '274' or @@form = '275' then 'BIDV.png'
					when @@form = '276' or @@form = '277' then 'Vietcombank.png'
					when @@form = '278' or @@form = '279' then 'Vietinbank.png'
					when @@form = '280' or @@form = '281' then 'EximBank.png'
					when @@form = '282' or @@form = '283' then 'MB.png'
					when @@form = '284' or @@form = '285' then 'Techcombank.png'
					when @@form = '286' or @@form = '287' then 'Agribank.png'
					when @@form = '288' or @@form = '289' then 'Sacombank.png'
					when @@form = '290' or @@form = '291' then 'DongA.png' else 'logo.png' end as file_name
end
]]>
    </text>
  </query>

